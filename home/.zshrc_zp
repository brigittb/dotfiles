#
#
# Executes commands at the start of an interactive session.
#

# Source Prezto.
if [[ -s "${ZDOTDIR:-$HOME}/.zprezto/init.zsh" ]]; then
  source "${ZDOTDIR:-$HOME}/.zprezto/init.zsh"
fi

alias lpowerline='. ~/Library/Python/2.7/lib/python/site-packages/powerline/bindings/zsh/powerline.zsh'
# fasd shorthands
alias f='fasd -f'
alias d='fasd -d'
alias z='fasd_cd -d' # mimic autojump
alias j=z
lpowerline
